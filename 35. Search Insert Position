// Runtime: 0 ms, faster than 100.00% 
// Memory Usage: 38.7 MB, less than 59.34%

class Solution {
    public int searchInsert(int[] nums, int target) {
        int start = 0;
        int end = nums.length - 1;
        while(start <= end){
            int mid = start + (end - start)/2;
            if(nums[mid] == target){
                return mid;
            }
            if(nums[mid] < target){
                start = mid + 1;
            }
            
            if(nums[mid] > target){
                end = mid - 1;
            }
        }
        
        return start;
    }
    
}
